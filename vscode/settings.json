{
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "q"],
      "after": [":", "q", "<CR>"]
    },
    {
      "before": ["<leader>", "w", "q"],
      "after": [":", "w", "q", "<CR>"]
    },
    {
      "before": ["<CR>"],
      "after": [":", "w", "<CR>"]
    },
    {
      "before": ["<leader>", "h"],
      "after": ["<C-w>", "h"]
    },
    {
      "before": ["<leader>", "j"],
      "after": ["<C-w>", "j"]
    },
    {
      "before": ["<leader>", "k"],
      "after": ["<C-w>", "k"]
    },
    {
      "before": ["<leader>", "l"],
      "after": ["<C-w>", "l"]
    },
    {
      "before": ["<leader>", "t"],
      "after": ["<C-t>"]
    },
    {
      "before": ["<leader>", "o"],
      "after": ["<C-o>"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["workbench.action.terminal.toggleTerminal"]
    },
    {
      "before": ["<leader>", "z"],
      "commands": ["workbench.action.minimizeOtherEditors"]
    },
    {
      "before": ["<leader>", "Z"],
      "commands": ["workbench.action.evenEditorWidths"]
    },
    {
      "before": ["<leader>", "r"],
      "after": ["\"", "q"]
    },
    {
      "before": ["<leader>", "p"],
      "after": ["<C-o>"]
    }
  ],
  "vim.leader": " ",
  "vs-kubernetes": {
    "vs-kubernetes.minikube-path": "/Users/ksi/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
  },
  "window.nativeTabs": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "editor.quickSuggestions": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
